# Local environment example - DO NOT commit real secrets here
# Copy this file to .env.local (or set env vars in your environment) and
# fill in the values for local development only.

# Enable the dev admin override (ONLY for local development/testing)
REACT_APP_DEV_ADMIN=false

# If you enable the dev admin override, set these to your local test admin
REACT_APP_DEV_ADMIN_EMAIL=admin@example.com
REACT_APP_DEV_ADMIN_PASSWORD=changeme

# Firebase config (example values) - set in your own environment
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
